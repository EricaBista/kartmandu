
 
  <div class="container">
  <div class="row">
    
        <div class="col-sm-2"></div>
                
                
                
                <div class="col-sm-8">
                <div class="signinLogo">
                <%= link_to image_tag("signinLogo.jpg"), root_path, :class => "img-responsive" %>
               <!--  <img class="img-responsive" src="assets/signinLogo.jpg"></div> -->
            
                
                
                <div class="row loginNsignup">
                
                <div class="col-sm-7">
                <div class="RegisterSignup">
                <p>Create your personal account </p>
                <h2>Register</h2>
                <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                  <%= devise_error_messages! %>

                <form>
                
                <div class="form-group">
                    Name
                    <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Enter your name">
                  </div>
                
                 <div class="form-group">
                    Address
                    <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Enter your address">
                  </div>
                  
                   <div class="form-group">
                    Phobe
                    <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Enter your phone">
                  </div>
                
                
               <div class="field">
                      <%= f.label :email %><br />
                      <%= f.email_field :email, autofocus: true ,:class=>'form-control'%>
                    </div>

                    <div class="field">
                      <%= f.label :password %>
                      <% if @validatable %>
                      <em>(<%= @minimum_password_length %> characters minimum)</em>
                      <% end %><br />
                      <%= f.password_field :password, autocomplete: "off",:class=>'form-control' %>
                    </div>

                    <div class="field">
                      <%= f.label :password_confirmation %><br />
                      <%= f.password_field :password_confirmation, autocomplete: "off",:class=>'form-control' %>
                    </div>

                    <div class="actions">
                      <%= f.submit "Sign up" %>
                    </div>

                 </form>

                 <% end %>
                </div>
                
                </div>
                
                
                
                
                
                <div class="col-sm-5 RegisterLogin">
                
                <p>Already have an account?</p>
                <%= form_for(:user, :url => user_session_path) do |f| %>
                 <%= devise_error_messages! %>
                <form>
               
           
        <div class="form-group">
            <%=label_tag :email %>
            <%#= text_field_tag :email, params[:email], :class=>'form-control' %>
            <%= f.text_field :email, :class=>'form-control' %>
          </div>
          <div class="form-group">
            <%=label_tag :password %> <br/>
          
            <%= f.password_field :password ,:class=>'form-control'%>
          </div>
         
        
         
          <div class="form-group">
          <%= label_tag :remember_me %>
          
          <%= f.check_box :remember_me %>
          </div> 
          <div class="form-group">
          <%= link_to "Forgot your password?", new_password_path(:user) %>
           </div>

          <%= render "devise/shared/links" %>
          <div class="form-group">
            <%=submit_tag "Log In", :class => "btn btn-success" %>
          </div>
       
        </form> 
        <% end %>
                
               
                
                </div>
                
                </div>
     
                </div>
       
                <div class="col-sm-2"></div>
  
  
    
    </div>
    
  </div>    
   </div> 
    
 
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="assets/bootstrap.min.js"></script>
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="assets/ie10-viewport-bug-workaround.js"></script>
 


